<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainProgController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="600.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" prefHeight="100.0" prefWidth="120.0" text="Play" textAlignment="CENTER" />
            <Button fx:id="bChats" alignment="CENTER" layoutX="120.0" mnemonicParsing="false" onAction="#openChatMenu" prefHeight="100.0" prefWidth="120.0" text="Chats" textAlignment="CENTER" />
            <Button fx:id="bSeatch" alignment="CENTER" layoutX="240.0" mnemonicParsing="false" onAction="#openSearchMenu" prefHeight="100.0" prefWidth="120.0" text="Search" textAlignment="CENTER" />
            <Button fx:id="bProfile" alignment="CENTER" layoutX="360.0" mnemonicParsing="false" onAction="#openProfileMenu" prefHeight="100.0" prefWidth="120.0" text="Profile" textAlignment="CENTER" />
            <Button fx:id="bLogOut" alignment="CENTER" layoutX="480.0" mnemonicParsing="false" onAction="#openLoginMenu" prefHeight="100.0" prefWidth="120.0" text="Log Out" textAlignment="CENTER" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
